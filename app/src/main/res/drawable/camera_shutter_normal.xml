<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape android:shape="oval">

            <padding android:left="2dp"
                android:top="2dp"
                android:right="2dp"
                android:bottom="2dp"/>

            <stroke
                android:width="0.5dp"
                android:color="#FFFFFF"/>
        </shape>
    </item>



    <item >
        <shape android:shape="oval">

            <padding android:left="4dp"
                android:top="4dp"
                android:right="4dp"
                android:bottom="4dp"/>


        </shape>
    </item>
    <item >
        <shape android:shape="oval">


            <solid
                android:color="#FFFFFF"
                />
            <size
                android:width="52dp"
                android:height="52dp"/>
        </shape>
    </item>

</layer-list>